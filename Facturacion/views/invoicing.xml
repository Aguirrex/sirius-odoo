<odoo>
    <record id="view_move_form_inherit_custom" model="ir.ui.view">
        <field name="name">account.move.form.inherit.custom</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/> <!-- ID de la vista original -->
        <field name="arch" type="xml">
            <!-- Ejemplo: insertar despuÃ©s del campo "partner_id" -->
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="tipo_factura"/>
            </xpath>
        </field>
    </record>
</odoo>